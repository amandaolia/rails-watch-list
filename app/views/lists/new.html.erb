<div class="container mt-3">
  <h1>New list</h1>
  <%= simple_form_for @list do |f| %>
    <%= f.input :name %>
    <%= f.file_field :image %>
    <%= f.submit "Create new List", class: "btn btn-primary" %>
  <% end %>
  <%= link_to "Back to my movie lists", lists_path, class: "btn btn-secondary mt-3 btn-light" %>
</div>
